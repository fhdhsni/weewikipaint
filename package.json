{
  "name": "weeWikiPaint",
  "version": "0.0.1",
  "description": "just following along James Shore from Let's Code: Test-Driven JavaScript",
  "author": "Farhad hsni <farhad.hsni@gmail.com>",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint ./test/",
    "rm": "rm -rf ./dist",
    "build": "yarn run mocha && webpack --config webpack.prod.config.js -p",
    "ws": "webpack-dev-server  --config webpack.dev.config.js --host $(ifconfig wlp3s0 | grep 'inet ' | awk '{ print $2 }')",
    "wsl": "webpack-dev-server --config webpack.dev.config.js",
    "tsc": "tsc ./src/server/*.ts --sourceMap -m 'CommonJS' -t 'ES6'",
    "premocha": "tsc ./test/*.ts --outDir ./test/ -m 'CommonJS' -t 'ES5'",
    "mocha": "mocha"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.4",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "extract-text-webpack-plugin": "beta",
    "html-webpack-inline-source-plugin": "^0.0.6",
    "html-webpack-plugin": "^2.26.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.4.0",
    "postcss-loader": "^1.2.2",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "tslint": "^4.4.2",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.1.5",
    "webpack": "beta",
    "webpack-dev-server": "beta"
  }
}
